{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "baseUrl": "./src",
      "paths": {
        "@lib/*": ["lib/*"]
      }
    },
    "include": ["src"]
  }